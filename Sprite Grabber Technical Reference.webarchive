img.thumb
{
	padding: 4px;
	border: 2px solid;

	margin-left: 4px;
	margin-right: 4px;
	
	border-color:#c0c0c0;
	background-color:#e0e0e0;
}


/*
	gel box effect on tables
*/



td.box_top
{
	background: url(../images/box_01.png);
	height: 44px;
}

td.box_left
{
	background: url(../images/box_02.png);
	width: 16px;
}

td.box_right
{
	background: url(../images/box_04.png);
	width: 16px;
}

td.box_bottom
{
	background: url(../images/box_05.png);
	height: 32px;
}

/*
	navbar colouring for active tab
*/

#navbar li a#current
{
	color:        #1d5673;
	background:   #1d5673;
	border-color: #1d5673;
}


